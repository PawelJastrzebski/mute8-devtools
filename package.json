{
  "name": "mute8-devtools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite --port 4030",
    "build": "tsc && vite build",
    "preview": "vite build && vite preview --port 4030",
    "mv:docs": "rm -rf ./docs && mv -f dist/  docs/",
    "deploy:preview": "vite preview --outDir docs --base /mute8-devtools/ --port 4030",
    "deploy": "npm run build && npm run mv:docs"
  },
  "dependencies": {
    "@alenaksu/json-viewer": "^2.0.1",
    "@minht11/solid-virtual-container": "^0.2.1",
    "@tanstack/solid-virtual": "^3.1.2",
    "@types/prismjs": "^1.26.3",
    "cors-window": "^0.0.0-alpha.4",
    "i": "^0.3.7",
    "monaco-editor": "^0.46.0",
    "mute8-plugins": "file:../x-js/packages/mute8-plugins",
    "mute8-solid": "^0.0.0-alpha.3",
    "npm": "^10.4.0",
    "pixi.js": "^7.4.0",
    "prismjs": "^1.29.0",
    "solid-js": "^1.8.7",
    "time-stamp": "^2.2.0"
  },
  "devDependencies": {
    "sass": "^1.70.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-solid": "^2.8.0"
  }
}
